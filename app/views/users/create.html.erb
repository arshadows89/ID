<div id="header" class="container-fluid">
	<div id="header-row" class="row">
		<div id="signupform" class='col-sm-12'>
			<% if @user.errors.any? %>
				<ul>
					<% @user.errors.full_messages.each do |message| %>
						<li><%= message %></li>
					<% end %>
				</ul>
			<% end %>
			<div id="signupformtext">
				<h1>Sign Up</h1>
				<%= yield :testing %>
				<%= render 'form' %>
			</div>
		</div>
	</div>
</div>