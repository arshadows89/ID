<div id="YoutubePage" class="row">
<% unless Video.all.empty? %>
  <% Video.all.sort_by {|vid| vid.id}.reverse.each do |vid| %>
	<div class='col-md-4'>
		<div id="youtubepagetext"><%= vid.text %></div>
		<iframe class="videoframe" src="<%= vid.link %>" frameborder="0" allowfullscreen></iframe>
	</div>
  <% end %>
<% end %>
</div>